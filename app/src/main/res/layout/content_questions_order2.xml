<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_questions_order2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="am.app.QuestionsOrder2"
    tools:showIn="@layout/activity_questions_order2">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18sp" />

    <TextView
        android:text="Score:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignRight="@+id/button16"
        android:layout_alignEnd="@+id/button16"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView15"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:id="@+id/textView21"
        android:textSize="18sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:id="@+id/textView13"
        android:textSize="18sp"
        android:layout_below="@+id/textView21"
        android:layout_centerHorizontal="true" />

    <RadioButton
        android:text="RadioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:id="@+id/Aa2"
        android:textSize="14sp"
        android:layout_below="@+id/textView13"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Cheat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button16"
        android:layout_below="@+id/Cc2"
        android:layout_toRightOf="@+id/Cc2"
        android:layout_toEndOf="@+id/Cc2"
        android:layout_marginTop="12dp" />

    <Button
        android:text="Skip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button15"
        android:layout_toLeftOf="@+id/Cc2"
        android:layout_toStartOf="@+id/Cc2"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp"
        android:layout_alignBottom="@+id/button16"
        android:layout_alignTop="@+id/button16" />

    <RadioButton
        android:text="RadioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:id="@+id/Bb2"
        android:layout_below="@+id/Aa2"
        android:layout_alignLeft="@+id/Aa2"
        android:layout_alignStart="@+id/Aa2" />

    <RadioButton
        android:text="RadioButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:id="@+id/Cc2"
        android:layout_below="@+id/Bb2"
        android:layout_alignLeft="@+id/Bb2"
        android:layout_alignStart="@+id/Bb2" />
</RelativeLayout>
